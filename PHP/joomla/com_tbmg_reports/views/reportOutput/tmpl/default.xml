<?xml version="1.0" encoding="utf-8"?>
<metadata>
        <layout title="Report From AWS.abpi Database">
                <message>Blasts Database on abpi.net Server</message>
        </layout>
        
        <fields name="request">
			<fieldset name="request">
				<field name="serverHost" type="list" default="" label="Database Server Location" description="">
				  <option value="local">localhost</option>
				  <option value="aws.abpi.net">aws.abpi</option>
				  <option value="assets.techbriefs.com">assets.techbriefs.com</option>
				</field>
	
				<field name="tableName" type="text" default="Table Name" label="Name of table" description="" size="10" />
				<field name="rows" type="text" default="" label="comma,seperated row list" description="" size="10" />
				<field name="tableHeader" type="text" default="" label="Header Above Reports Table" description="" size="10" />
	
				
				
		</fieldset>
	</fields>
</metadata>